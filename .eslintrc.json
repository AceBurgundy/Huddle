{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "max-len": ["error", { "code": 150, "tabWidth": 2, "comments": 200 }],
    "arrow-parens": ["error", "as-needed"],
    "comma-dangle": ["error", "never"],
    "no-unused-vars": ["error"]
  }
}
